{
  "author": {
    "name": "Rick Osborne",
    "url": "https://rickosborne.org"
  },
  "dependencies": {
    "@slack/rtm-api": "6.0.0",
    "@slack/web-api": "6.7.0",
    "aws-lambda": "1.0.7",
    "aws-sdk": "2.1091.0",
    "inclined-plane": "0.5.3",
    "mysql2": "2.3.3",
    "rxjs": "7.5.5",
    "twitter": "1.7.1",
    "xmlhttprequest": "1.8.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.93",
    "@types/aws-sdk": "2.7.0",
    "@types/chai": "4.3.0",
    "@types/ejs": "3.1.0",
    "@types/mocha": "9.1.0",
    "@types/mysql2": "github:types/mysql2#semver:1.0.0",
    "@types/node": "17.0.21",
    "@types/twitter": "1.7.1",
    "bufferutil": "4.0.6",
    "cardinal": "2.1.1",
    "chai": "4.3.6",
    "mocha": "9.2.1",
    "nyc": "15.1.0",
    "ts-loader": "9.2.8",
    "ts-mocha": "9.0.2",
    "ts-node": "10.7.0",
    "tslint": "6.1.3",
    "typescript": "4.6.2",
    "utf-8-validate": "5.0.9",
    "webpack": "5.70.0",
    "webpack-cli": "4.9.2",
    "webpack-node-externals": "3.0.0",
    "wtfnode": "0.9.1"
  },
  "name": "scrubjay",
  "private": true,
  "scripts": {
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "clean": "rm -Rf dist",
    "compile": "tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "nuke": "rm -Rf node_modules && rm -Rf package-lock.json && npm install && npm audit fix",
    "package": "npm run package:feeder && npm run package:song && npm run package:slash-lambda",
    "package:scrubjay": "NODE_OPTIONS='--max_old_space_size=2048' webpack-cli --config song.webpack.config.js",
    "package:slash-lambda": "NODE_OPTIONS='--max_old_space_size=2048' webpack-cli --config slash-lambda.webpack.config.js",
    "package:song": "NODE_OPTIONS='--max_old_space_size=2048' webpack-cli --config song.webpack.config.js",
    "package:feeder": "NODE_OPTIONS='--max_old_space_size=2048' webpack-cli --config feeder.webpack.config.js",
    "precompile": "npm run clean && npm run lint",
    "prepackage": "npm run clean && npm run lint",
    "start": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' node ./node_modules/.bin/ts-node scrubjay.ts",
    "start:feeder": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' node ./node_modules/.bin/ts-node feeder.ts",
    "start:song": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' node ./node_modules/.bin/ts-node song.ts",
    "test": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' node ./node_modules/.bin/ts-mocha --timeout 30000 test/**/*.ts",
    "un-prestart": "npm run package"
  },
  "version": "0.5.20"
}
