{
  "author": {
    "name": "Rick Osborne",
    "url": "https://rickosborne.org"
  },
  "dependencies": {
    "@slack/rtm-api": "^5.0.5",
    "@slack/web-api": "^5.13.0",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.780.0",
    "inclined-plane": "^0.5.3",
    "mysql2": "^2.2.5",
    "rxjs": "^6.6.3",
    "twitter": "^1.7.1",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.64",
    "@types/aws-sdk": "^2.7.0",
    "@types/chai": "^4.2.14",
    "@types/ejs": "^3.0.5",
    "@types/mocha": "^8.0.3",
    "@types/mysql2": "github:types/mysql2",
    "@types/node": "^14.14.5",
    "@types/twitter": "^1.7.0",
    "bufferutil": "^4.0.1",
    "cardinal": "^2.1.1",
    "chai": "^4.2.0",
    "mocha": "^8.2.0",
    "nyc": "^15.1.0",
    "ts-loader": "^8.0.7",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.5",
    "utf-8-validate": "^5.0.2",
    "webpack": "^5.3.0",
    "webpack-cli": "^4.1.0",
    "webpack-node-externals": "^2.5.2",
    "wtfnode": "^0.8.3"
  },
  "name": "scrubjay",
  "private": true,
  "scripts": {
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "clean": "rm -Rf dist",
    "compile": "tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "package": "npm run package:scrubjay && npm run package:feeder && npm run package:song && npm run package:slash-lambda",
    "package:scrubjay": "NODE_OPTIONS='--max_old_space_size=1024' webpack-cli --config song.webpack.config.js",
    "package:slash-lambda": "NODE_OPTIONS='--max_old_space_size=1024' webpack-cli --config slash-lambda.webpack.config.js",
    "package:song": "NODE_OPTIONS='--max_old_space_size=1024' webpack-cli --config song.webpack.config.js",
    "package:feeder": "NODE_OPTIONS='--max_old_space_size=1024' webpack-cli --config feeder.webpack.config.js",
    "precompile": "npm run clean && npm run lint",
    "prepackage": "npm run clean && npm run lint",
    "start": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' node ./node_modules/.bin/ts-node scrubjay.ts",
    "start:feeder": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' node ./node_modules/.bin/ts-node feeder.ts",
    "start:song": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' node ./node_modules/.bin/ts-node song.ts",
    "test": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' node ./node_modules/.bin/ts-mocha --timeout 30000 test/**/*.ts",
    "un-prestart": "npm run package"
  },
  "version": "0.5.3"
}
