  "name": "scrubjay",
  "private": true,
  "scripts": {
    "clean": "rm -Rf dist",
    "compile": "tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "package": "webpack-cli",
    "precompile": "npm run clean && npm run lint",
    "prepackage": "npm run clean && npm run lint",
    "un-prestart": "npm run package",
    "start": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' node $NODE_DEBUG_OPTION ./node_modules/.bin/ts-node scrubjay.ts",
    "test": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' node $NODE_DEBUG_OPTION ./node_modules/.bin/ts-mocha test/**/*.ts"
  },
  "version": "0.1.0"
}
